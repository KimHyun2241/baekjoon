   "source": [
    "# 최대공약수(gcd) 계산\n",
    "def gcd(a, b):\n",
    "    for i in range(1, min(a, b)):\n",
    "        if (a % i == 0 and b % i == 0):\n",
    "            gcd_val = i\n",
    "    return gcd_val\n",
    "\n",
    "\n",
    "# 최소공배수(lcm) 계산\n",
    "def lcm(a, b):\n",
    "    return a * b // gcd(a, b)\n",
    "\n",
    "a,b = map(int, input().split())\n",
    "print(gcd(a, b))\n",
    "print(lcm(a, b))"
   ]
