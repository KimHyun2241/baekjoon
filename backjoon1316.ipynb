   "source": [
    "n = int(input())\n",
    "count = n\n",
    "\n",
    "for i in range(n):\n",
    "    word = input()\n",
    "    for j in range(len(word)-1):\n",
    "        if word[j] != word[j+1]: #같은 문자열이 연속한지 체크\n",
    "            if word[j] in word[j+1:]: # 같은 문자열이 j+1: 이후로 있을 경우 count -=1\n",
    "                count -= 1\n",
    "                break\n",
    "\n",
    "print(count)"
   ]
